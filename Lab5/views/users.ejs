<!DOCTYPE html>
<html>
  <head>
    <title>Users</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
      <h2> List of Users </h2> <hr />
      <table>
        <tr>
            <th>Id</th>
            <th>Name</th> 
            <th>User name</th>
            <th>Email</th>
            <th>City</th>
            <th>Zip code</th>
            <th>Latitude</th>
            <th>Longitude</th>
            <th>Website</th>
            
          </tr>
          <% for(var i=0; i < users.length; i++) { %>
            <tr>
              <td><%= users[i].id %></td>
              <td><%= users[i].name %></td>
              <td><%= users[i].username %></td>
              <td><%= users[i].email %></td>
              <td><%= users[i].address.city %></td>
              <td><%= users[i].address.geo.lat %></td>
               <td><%= users[i].address.geo.lng %></td>
              <td><%= users[i].website %></td>
            </tr>
          <% } %>     
      </table>
  </body>
</html>
